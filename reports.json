{
   "name":"Custom_Project",
   "readme_file":"README.md",
   "version":"1.0.0",
   "language":"python",
   "reports":[
      {
         "name":"tutorial_report",
         "readme_file":"reports/product_items/README.md",
         "template":"reports/product_items/templates/xlsx/template.xlsx",
         "start_row":2,
         "start_col":1,
         "entrypoint":"reports.product_items.entrypoint.generate",
         "audience":[
            "provider",
            "vendor"
         ],
         "report_spec":"1",
         "parameters":[
            {
               "id":"date",
               "type":"date_range",
               "name":"Report period",
               "description":"Provide the time period to create the report",
               "required":true
            },
            {
               "id":"product",
               "type":"product",
               "name":"Product List",
               "description":"Select the products you want to include in report",
               "required":true
            }
         ]
      }
   ]
}
